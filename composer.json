{
  "name": "drsdre/yii2-add-to-homescreen",
  "description": "Yii2 asset extension for https://github.com/cubiq/add-to-homescreen",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","asset","widget","mobile","homescreen","favorite"],
  "license": "Apache-2.0",
  "homepage": "https://github.com/drsdre/yii2-add-to-homescreen",
  "support": {
    "issues": "https://github.com/drsdre/yii2-add-to-homescreen/issues?state=open",
    "source": "https://github.com/drsdre/yii2-add-to-homescreen",
    "docs": "http://cubiq.org/add-to-home-screen"
  },
  "authors": [
    {
      "name": "Andre Schuurman",
      "email": "andre.schuurman+yii2-add-to-homescreen@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0",
    "bower-asset/add-to-homescreen": "~3.2"
  },
  "autoload": {
    "psr-4": {
      "AddToHomescreen\\": ""
    }
  },
  "extra": {
    "asset-optimize-with-installed-packages": false,
    "asset-repositories": {
      "bower-asset/add-to-homescreen": {
        "type": "bower-github",
        "url": "https://github.com/cubiq/add-to-homescreen.git"
      }
    }
  }
}